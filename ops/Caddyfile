{
    admin off
}

localhost {
    log {
        output stdout
        format json
        level DEBUG
    }
    @websockets {
        header Connection *Upgrade*
        header Upgrade websocket
    }
    reverse_proxy @websockets {args.0}:5000 {args.0}:5001
    reverse_proxy {args.0}:5000 {args.0}:5001
}

